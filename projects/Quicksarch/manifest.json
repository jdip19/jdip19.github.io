{
  "manifest_version": 3,
  "name": "Quicksarch",
  "description": "Search results on multiple sites from one input. specially for designer and developers.",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
  
}